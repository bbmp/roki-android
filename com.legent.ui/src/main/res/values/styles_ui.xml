<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="text">
        <item name="android:textColor">@color/common_text_color_selector</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">false</item>
        <item name="android:gravity">left|center_vertical</item>
    </style>

    <style name="text_title" parent="@style/text">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/Black</item>
    </style>

    <style name="text_content" parent="@style/text">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#565555</item>
    </style>

    <style name="text_small" parent="@style/text">
        <item name="android:textSize">@dimen/common_text_size_small</item>
    </style>

    <style name="common_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/common_button_height</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>s
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:background">@drawable/common_button_background_selector</item>
        <item name="android:textColor">@color/Black</item>
        <item name="android:textSize">@dimen/common_button_text_size</item>
        <item name="android:text">@string/common_ok</item>
    </style>


    <style name="common_listview" parent="@android:style/Widget.Holo.Light.ListView">
        <item name="android:cacheColorHint">@color/Transparent</item>
        <item name="android:listSelector">@color/Transparent</item>
        <item name="android:divider">@color/common_divide_background</item>
        <item name="android:dividerHeight">1dp</item>
        <item name="android:dividerPadding">0dp</item>
        <item name="android:fadingEdge">none</item>
        <item name="android:fastScrollEnabled">false</item>
        <item name="android:scrollingCache">false</item>
        <item name="android:scrollbarStyle">outsideOverlay</item>
        <item name="android:smoothScrollbar">true</item>
        <item name="android:headerDividersEnabled">false</item>
    </style>

    <style name="common_wifi_listview" parent="@android:style/Widget.Holo.Light.ListView">
        <item name="android:cacheColorHint">@color/Transparent</item>
        <item name="android:listSelector">@color/Transparent</item>
        <item name="android:divider">#ececec</item>
